                    <operator>EQUAL</operator>
                    <rightValue>Sales</rightValue>
                </criteria>
                <criteria>
                    <leftValue>{!$User.UserRole.Name}</leftValue>
                    <operator>CONTAINS</operator>
                    <rightValue>Sales</rightValue>
