            <componentName>flexipage:richText</componentName>
            <visibilityRule>
                <booleanFilter>(1 OR 5) AND (2 OR 3 OR 4)</booleanFilter>
                <criteria>
                    <leftValue>{!Record.TickerSymbol}</leftValue>
                    <operator>CONTAINS</operator>
                    <rightValue>LON:</rightValue>
                </criteria>
                <criteria>
                    <leftValue>{!$User.UserRole.Name}</leftValue>
                    <operator>CONTAINS</operator>
                    <rightValue>Sales</rightValue>
                </criteria>
                <criteria>
                    <leftValue>{!$User.Profile.Name}</leftValue>
                    <operator>EQUAL</operator>
                    <rightValue>Sales</rightValue>
