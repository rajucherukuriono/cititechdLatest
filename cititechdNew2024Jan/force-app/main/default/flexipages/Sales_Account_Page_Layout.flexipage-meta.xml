                    <leftValue>{!$User.UserRole.Name}</leftValue>
                    <operator>EQUAL</operator>
                    <rightValue>System Administrator</rightValue>
                </criteria>
                <criteria>
                    <leftValue>{!Record.Exchange__c}</leftValue>
                    <operator>EQUAL</operator>
                    <rightValue>LONDON</rightValue>
                </criteria>
            </visibilityRule>
        </componentInstances>
