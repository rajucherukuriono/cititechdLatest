                <criteria>
                    <leftValue>{!$User.Profile.Name}</leftValue>
                    <operator>EQUAL</operator>
                    <rightValue>Sales</rightValue>
                </criteria>
                <criteria>
                    <leftValue>{!$User.UserRole.Name}</leftValue>
                    <operator>EQUAL</operator>
                    <rightValue>System Administrator</rightValue>
                </criteria>
                <criteria>
                    <leftValue>{!Record.Account.Exchange__c}</leftValue>
                    <operator>EQUAL</operator>
                    <rightValue>LONDON</rightValue>
                </criteria>
            </visibilityRule>
        </componentInstances>
