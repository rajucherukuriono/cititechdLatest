    <rules>
        <fullName>Client Confirmed Alert</fullName>
        <actions>
            <name>Finance_Client_Confirmed</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Order.Status</field>
            <operation>equals</operation>
            <value>Client Confirmed</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Deal Roadshow Capital Markets Delivered</fullName>
        <actions>
            <name>Deal_Roadshow_Capital_Markets_Delivered</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Order.Status</field>
            <operation>equals</operation>
            <value>Delivered</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Event_Type_OE_Defined__c</field>
            <operation>equals</operation>
            <value>Deal Roadshow</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Business_Line__c</field>
            <operation>equals</operation>
            <value>Capital Markets</value>
        </criteriaItems>
        <description>For Capital Markets business line, any time the event type is deal roadshow notify Cathy and Gary.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Order Delivered Status 24hours%2B</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Order.Status</field>
            <operation>equals</operation>
            <value>Delivered</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Client_Confirmed_Date_Time_Stamp__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Order.Business_Line__c</field>
            <operation>notEqual</operation>
            <value>Corporate StreamLinks</value>
        </criteriaItems>
        <description>7/21 - Per Heather Allen and Finance, this rule deactivated for now. 

If the order status has been on Delivered Status for more than 24 hours, this will trigger an email to the OE Business Lead to make sure to update to Client Confirmed.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Order_Stuck_on_Delivered</name>
                <type>Alert</type>
            </actions>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Order In Progress Null Products</fullName>
        <active>false</active>
        <description>Number of total Products associated to this Order is at 0. Email alert to notify OE Business Lead to add Products ASAP.</description>
        <formula>AND(
ISPICKVAL(Status,&apos;In Progress&apos;) &amp;&amp;
Number_of_Total_Products_Quantity__c = 0.00 &amp;&amp;
TotalAmount = 0.00
)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Order_is_In_Progress_but_with_no_Products</name>
                <type>Alert</type>
            </actions>
            <timeLength>2</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Order_is_In_Progress_but_with_no_Products_6_days</name>
                <type>Alert</type>
            </actions>
            <timeLength>6</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>Order_is_In_Progress_but_with_no_Products_4_days</name>
                <type>Alert</type>
            </actions>
            <timeLength>4</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Order within 48 hours at New Status</fullName>
        <actions>
            <name>Order_is_still_New_status_and_only_48_hours_to_Start</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>Order within 48 hours at New Status</description>
        <formula>AND(
DATEVALUE(CreatedDate) &lt;&gt; TODAY(),
ISPICKVAL(Status,&apos;New&apos;)
)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <offsetFromField>Order.EffectiveDate</offsetFromField>
            <timeLength>-2</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
