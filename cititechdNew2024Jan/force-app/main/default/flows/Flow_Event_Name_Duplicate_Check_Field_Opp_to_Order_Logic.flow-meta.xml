    <formulas>
        <name>Enddatetime</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE({!$Record.Opportunity.Event_End_Date__c}) + 0.75</expression>
    </formulas>
    <formulas>
        <name>Hybridevent</name>
        <dataType>String</dataType>
        <expression>IF(INCLUDES({!$Record.Opportunity.Expected_Services__c}, &quot;Hybrid&quot;),&quot;Yes&quot;,&quot;No&quot;)</expression>
    </formulas>
    <formulas>
        <name>orderamountopp</name>
        <dataType>Number</dataType>
        <expression>{!$Record.Dhruvsoft__OrderAmtFromOpportunityAmt__c}/4</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
