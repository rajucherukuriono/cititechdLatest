        <rules>
            <name>Update_Order_Amount_Opportunity_Institutional_Business_to_0</name>
            <conditionLogic>1 OR 4 OR 5 OR (2 AND 3)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Opportunity.RecordType.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Institutional Business</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Opportunity.Owner.LastName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Garrigan</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Opportunity.Channel_Agency_Referral__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
